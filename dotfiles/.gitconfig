[color]
  diff = auto
  status = auto
  branch = auto
  ui = true
[color "status"]
  changed = blue normal bold
  added = yellow normal bold
  untracked = cyan normal bold
[color "branch"]
  upstream = blue normal bold
  local = cyan normal bold
  remote = bold red
[color "diff"]
  plain = white
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red blink
[alias]
  prettylog = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold green)(%ar)%C(reset) %C(white)%s%C(reset) %C(dim white)- %an%C(reset)%C(bold yellow)%d%C(reset)' --all
[push]
  default = simple
